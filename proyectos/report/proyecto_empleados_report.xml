<odoo>
    <record id="proyectos.empleados_report" model="ir.actions.report">
        <field name="name">Informe empleados</field>
        <field name="model">proyectos.departamento</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">proyectos.departamento_report_view</field>
        <field name="report_file">proyectos.departamento_report</field>
        <field name="binding_model_id" ref="model_proyectos_departamento"/>
        <field name="binding_type">report</field>
    </record>


    <template id="departamento_report_view">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Report title</h2>
                    </div>
                </t>
            </t>
        </t>

    </template>
    

</odoo>